# Each field in this block is identified by a unique <element> <level> pair.
# Each element name must correspond to an element name defined in the
# "Elements" block below.
# Similarly, each level name must correspond to a level name defined in the
# "Levels" block below.
# (Note that all name comparisons are case insensitive!)
#
################################################################################

Fields
	{

	pressure		msl
		{
		label			= <*default*> "Msl Pressure"
		short_label		= <*default*> "Msl Pres"
		}

	temperature		surface
		{
		label			= <*default*> "Surface Temperature"
		short_label		= <*default*> "Sfc Temp"
		}

	mixing_ratio	surface
		{
		element_info	=
			{
			equation		=
				{
				equation_string		= EPSR * svprs[td] \
											/ (psfc - svprs[td])
				equation_units		= kg/kg
				}
			}
		}

	virtual_temp	surface
		{
		element_info	=
			{
			equation		=
				{
				equation_string		= temp / (1 - (1-EPSR)*svprs[td]/psfc)
				equation_units		= degreesK
				}
			}
		}

	air_density		surface
		{
		element_info	=
			{
			equation		=
				{
				equation_string		= psfc / (RDRY * tvirt)
				equation_units		= unitless
				}
			}
		}

	theta_e			surface
		{
		element_info	=
			{
			equation		=
				{
				equation_string		= temp * pow[100000/psfc, \
											0.2854 * (1.0 - 0.00028*wrat)] \
											* exp[(3.376/tlcl - 0.00254) \
											* wrat * (1.0 + 0.00081*wrat)]
				equation_units		= degreesK
				}
			}
		}

