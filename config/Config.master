################################################################################
# Formula for condensation_temp from Bolton, MWR 108, July 1980, 1046-1053
################################################################################

	condensation_temp
		{
		label			= <*default*> "Lifting Condensation Level Temp"
		short_label		= <*default*> "LCL Temp"
		element_group	= Temperature
		level_type		= Level
		alias			= lifting_condensation_level_temperature  tlcl

		file_id			= None
		file_ident		= None
		precision		= .01  MKS

		time_dependence	= Normal

		field_type		= Continuous
		wind_class		= None

		editor			= None

		sample			= Default

		equation		=
			{
			equation_string		= 1 / (1/(temp-55) - log[rh]/2840) + 55
			equation_units		= degreesK
			}
		}

################################################################################
# Formula for theta_e from Bolton, MWR 108, July 1980, 1046-1053
################################################################################

	theta_e
		{
		label			= <*default*> "Equivalent Potential Temp"
		short_label		= <*default*> "ThetaE"
		element_group	= Temperature
		level_type		= Level
		alias			= equivalent_potential_temp  thetae

		file_id			= te
		file_ident		= theta_e
		precision		= .01  Kdegrees

		time_dependence	= Normal

		field_type		= Continuous
		wind_class		= None

		editor			= None

		sample			= Default

		equation		=
			{
			equation_string		= temp * pow[100000/lvlprs[], \
										0.2854 * (1.0 - 0.00028*wrat)] \
										* exp[(3.376/tlcl - 0.00254) \
										* wrat * (1.0 + 0.00081*wrat)]
			equation_units		= degreesK
			}
		}

